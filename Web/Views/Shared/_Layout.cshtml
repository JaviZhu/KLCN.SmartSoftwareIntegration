    <script type="text/javascript">
        $(document).ready(function () {
            var menu = $("#hid_menu").val().split('-');
            $("#sidebar-menu").find("ul").find("li:eq(0)").find("a").removeClass();
            if (menu[1] == 1) {
                $("#sidebar-menu").find("ul:eq(0)").find("li:eq(" + menu[0] + ")").find("a:eq(0)").removeClass().addClass("waves-effect waves-light subdrop");
                $("#sidebar-menu").find("ul:eq(0)").find("li:eq(" + menu[0] + ")").find("ul").css("display", "block");
                $("#sidebar-menu").find("ul:eq(0)").find("li:eq(" + menu[0] + ")").find("ul li:eq(" + menu[2] + ")").find("a").css("color", "#2b9ac9");
                $("#sidebar-menu").find("ul:eq(0)").find("li:eq(" + menu[0] + ")").find("a:eq(0)").find("span:eq(1)").find("i").removeClass().addClass("md md-remove");
            } else
                $("#sidebar-menu").find("ul:eq(0)").find("li:eq(" + menu[0] + ")").find("a").removeClass().addClass("waves-effect waves-light active");
            $("#copyright").text("Copyright Klueber 2018");

            $("#displayname").text($.cookie("displayname"));
            $("#title").text($.cookie("title"));
        });
    </script>
